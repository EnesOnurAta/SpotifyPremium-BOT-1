const {RichEmbed} = require("discord.js");
exports.run = async (client, message, args, level) => {
if (message.author.id == "274551537139712001") {
var nameResult = args.join(' ');
if (!nameResult) nameResult = null;
client.user.setActivity(nameResult, {type: "STREAMING"});
let embed = new RichEmbed()
.setAuthor(`${message.author.tag}`, `${message.author.avatarURL}`)
.setDescription(`${nameResult} başarıyla botun yayını ayarlandı (Durum: STREAMING)`)
message.channel.send(embed)
} else {
  let embed2 = new RichEmbed()
  .setTitle("Bu komutu sadece sahibim kullanabilir")
  message.channel.send(embed2)
}
}
exports.conf = {
  enabled: true,
  guildOnly: false,
  aliases: [],
  permLevel: "Bot Sahibi"
};

exports.help = {
  name: "durumyayın",
  category: "admin",
  description: "Botun durumunu yayın yapar",
  usage: "durumyayın"
};
